#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ts/workspace/timothy.engineering-astro/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier-plugin-astro@0.12.0_prettier@3.0.3_typescript@5.2.2/node_modules/@astrojs/language-server/bin/node_modules:/Users/ts/workspace/timothy.engineering-astro/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier-plugin-astro@0.12.0_prettier@3.0.3_typescript@5.2.2/node_modules/@astrojs/language-server/node_modules:/Users/ts/workspace/timothy.engineering-astro/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier-plugin-astro@0.12.0_prettier@3.0.3_typescript@5.2.2/node_modules/@astrojs/node_modules:/Users/ts/workspace/timothy.engineering-astro/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier-plugin-astro@0.12.0_prettier@3.0.3_typescript@5.2.2/node_modules:/Users/ts/workspace/timothy.engineering-astro/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ts/workspace/timothy.engineering-astro/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier-plugin-astro@0.12.0_prettier@3.0.3_typescript@5.2.2/node_modules/@astrojs/language-server/bin/node_modules:/Users/ts/workspace/timothy.engineering-astro/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier-plugin-astro@0.12.0_prettier@3.0.3_typescript@5.2.2/node_modules/@astrojs/language-server/node_modules:/Users/ts/workspace/timothy.engineering-astro/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier-plugin-astro@0.12.0_prettier@3.0.3_typescript@5.2.2/node_modules/@astrojs/node_modules:/Users/ts/workspace/timothy.engineering-astro/node_modules/.pnpm/@astrojs+language-server@2.5.2_prettier-plugin-astro@0.12.0_prettier@3.0.3_typescript@5.2.2/node_modules:/Users/ts/workspace/timothy.engineering-astro/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@astrojs/language-server/bin/nodeServer.js" "$@"
else
  exec node  "$basedir/../@astrojs/language-server/bin/nodeServer.js" "$@"
fi
